{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      false
    ],
    "interface-name": false,
    "prefer-for-of": false,
    "object-literal-sort-keys": false,
    "variable-name": [ true,
      "ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"
    ],
    "array-type": [
      false
    ]
  },
  "rulesDirectory": [
  ]
}
