{
  "name": "sigil.ts",
  "version": "0.0.10",
  "description": "Minimalistic typescript object manipulation library",
  "main": "dist/cjs/sigil.js",
  "typings": "dist/sigil.d.ts",
  "module": "dist/sigil.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdrel/sigil.ts"
  },
  "scripts": {
    "lint": "tslint -p src",
    "prebuild": "rimraf dist",
    "build": "tsc -p src && tsc -p src -m commonjs --outDir dist/cjs",
    "test": "jest",
    "start": "jest --watch"
  },
  "keywords": [
    "object",
    "schema",
    "json",
    "typescript",
    "underscore",
    "lodash"
  ],
  "author": "Alex Drel",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
